{
    "name": "klintlili/easier-databases",
    "description": "Base is a simple library that makes it easier to work with mysql databases in PHP",
    "keywords": ["php", "mysql", "composer", "project template"],
    "homepage": "https://github.com/klintlili/easier-databases/",
    "type": "library",
    "license": "MIT",
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "klintlili",
            "email": "909102757@qq.com",
            "homepage": "https://www.xiongxi.net/"
        }
    ],
    "require": {
        "php": ">=5.6.0"
    },
    "config": {
        "process-timeout": 1800,
        "fxp-asset": {
            "enabled": false
        }
    },
    "autoload": {
        "psr-4": { "klintlili\\base\\": "src" }
    },
    "repositories": {
        "asset-packagist": {
            "type": "composer",
            "url": "https://asset-packagist.org"
        },
        "packagist": {
            "type": "composer",
            "url": "https://mirrors.aliyun.com/composer/"
        }
    }
}
